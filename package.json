{
    "name": "lib.sbmr.in",
    "version": "0.1.0",
    "author": "YamaD <yamad@submarin.online>",
    "devDependencies": {
        "cjp": "1.x",
        "parcel-bundler": "1.x",
        "parcel-plugin-greasemonkey": "1.x"
    },
    "license": "MIT",
    "repository": "git@github.com:DevSubmarinonline/lib.sbmr.in",
    "scripts": {
        "build": "npm run build:static && npm run build:cjp && npm run build:deck",
        "build:cjp": "parcel build -d pub src/cjp/cjp.js",
        "build:deck": "cd src/deck/ && parcel build -d ../../pub --no-source-maps *.js",
        "build:static": "mkdir -p pub && cp -r static/* pub/"
    }
}
